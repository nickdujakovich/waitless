    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="HostnoMore.Views.MenuOneContainerPage"
    xmlns:views="clr-namespace:HostnoMore.Views">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Icon="shoppingcart.png" Command="{Binding GoToCart}"/>
    </TabbedPage.ToolbarItems>
    <views:ComboPage1
        Title="Combos"
        Icon="meal.png"
        />
    <views:EntreeSelectionPage1
        Title="Entrees"
        Icon="indivitem.png"
        />
    <views:SidePage1
        Title="drinks"
        Icon="drink.png"
        />
</TabbedPage>
